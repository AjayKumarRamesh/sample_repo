            steps {
                echo 'Hello World'
            }
        }
        stage('deloy') {
            steps {
                echo 'deploying'
            }
        }
        stage('release') {
            steps {
                echo 'releasing'
            }
        }
        stage('done') {
            steps {
                echo 'done'
            }
        }
    }
}
